{
  "name": "nodejstypescript",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "start": "npm run build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.js",
    "style:check": "prettier --check 'src/*/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/*/*.ts' 'test/**/*.ts'",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janderrv/nodejstypescript.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/janderrv/nodejstypescript/issues"
  },
  "homepage": "https://github.com/janderrv/nodejstypescript#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.39",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  }
}
